<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_bg">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_bg"
        android:orientation="vertical">

        <include layout="@layout/include_topbar" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
                android:id="@+id/pullToRefresh"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_above="@id/rl_holder_bottom"
                android:visibility="gone">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <androidx.core.widget.NestedScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scrollbars="none">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/rv_flat_images"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:orientation="horizontal"
                                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                tools:itemCount="4"
                                tools:listitem="@layout/item_profile_image" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="@dimen/_10sdp"
                                android:layout_marginVertical="@dimen/_20sdp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1.2"
                                        android:orientation="vertical">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <ImageView
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/ic_location" />

                                            <TextView
                                                android:id="@+id/txt_distance"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginStart="@dimen/_5sdp"
                                                android:fontFamily="@font/font_regular"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_12sdp" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/_5sdp"
                                            android:gravity="center"
                                            android:orientation="horizontal">

                                            <ImageView
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/ic_fscore" />

                                            <TextView
                                                android:id="@+id/txt_flat_score"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginStart="@dimen/_5sdp"
                                                android:fontFamily="@font/font_regular"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_12sdp" />

                                        </LinearLayout>

                                        <com.joinflatshare.customviews.CustomButton
                                            android:id="@+id/btn_flat_map"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/_5sdp"
                                            android:background="@drawable/drawable_button_blue"
                                            android:fontFamily="@font/font_regular"
                                            android:minHeight="0dp"
                                            android:paddingHorizontal="@dimen/_5sdp"
                                            android:paddingVertical="@dimen/_5sdp"
                                            android:text="View on Map"
                                            android:textColor="@color/white"
                                            android:textSize="@dimen/_8sdp"
                                            android:visibility="gone" />

                                    </LinearLayout>


                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="0.8"
                                        android:gravity="center_vertical|end"
                                        android:orientation="vertical">

                                        <LinearLayout
                                            android:id="@+id/ll_rent"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/font_regular"
                                                android:text="Rent"
                                                android:textColor="@color/color_hint"
                                                android:textSize="@dimen/_12sdp" />

                                            <TextView
                                                android:id="@+id/txt_rent"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/font_semibold"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:id="@+id/ll_deposit"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/font_regular"
                                                android:text="Deposit"
                                                android:textColor="@color/color_hint"
                                                android:textSize="@dimen/_12sdp" />

                                            <TextView
                                                android:id="@+id/txt_deposit"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/font_semibold"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_12sdp" />
                                        </LinearLayout>
                                    </LinearLayout>
                                </LinearLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/line_height"
                                    android:layout_marginVertical="@dimen/_10sdp"
                                    android:background="@color/divider" />

                                <LinearLayout
                                    android:id="@+id/ll_flatmates"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/font_semibold"
                                        android:text="Flatmates"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_14sdp" />

                                    <androidx.recyclerview.widget.RecyclerView
                                        android:id="@+id/rv_flat_member"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_10sdp"
                                        android:orientation="horizontal"
                                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                        tools:itemCount="4"
                                        tools:listitem="@layout/item_flatmember" />

                                </LinearLayout>

                                <com.google.android.material.card.MaterialCardView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    app:cardBackgroundColor="@color/color_icon"
                                    app:cardCornerRadius="@dimen/_10sdp"
                                    app:cardElevation="0dp"
                                    app:cardUseCompatPadding="false">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="@dimen/_10sdp"
                                        android:orientation="vertical">

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentStart="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_regular"
                                                android:text="Flat Type"
                                                android:textColor="@color/color_grey_icon"
                                                android:textSize="@dimen/_12sdp" />

                                            <TextView
                                                android:id="@+id/txt_flatsize"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentEnd="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_bold"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp" />
                                        </RelativeLayout>

                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/line_height"
                                            android:layout_marginVertical="@dimen/_10sdp"

                                            android:background="@color/divider" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentStart="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_regular"
                                                android:text="Room Type"
                                                android:textColor="@color/color_grey_icon"
                                                android:textSize="@dimen/_12sdp" />

                                            <TextView
                                                android:id="@+id/txt_roomtype"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentEnd="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_bold"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp" />
                                        </RelativeLayout>

                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/line_height"
                                            android:layout_marginVertical="@dimen/_10sdp"
                                            android:background="@color/divider" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentStart="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_regular"
                                                android:text="Gender"
                                                android:textColor="@color/color_grey_icon"
                                                android:textSize="@dimen/_12sdp" />

                                            <TextView
                                                android:id="@+id/txt_gender"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentEnd="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_bold"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp" />
                                        </RelativeLayout>

                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/line_height"
                                            android:layout_marginVertical="@dimen/_10sdp"
                                            android:background="@color/divider" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentStart="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_regular"
                                                android:text="Available By"
                                                android:textColor="@color/color_grey_icon"
                                                android:textSize="@dimen/_12sdp" />

                                            <TextView
                                                android:id="@+id/txt_movein"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentEnd="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_bold"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp" />
                                        </RelativeLayout>

                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/line_height"
                                            android:layout_marginVertical="@dimen/_10sdp"
                                            android:background="@color/divider" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentStart="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_regular"
                                                android:text="Furnishing"
                                                android:textColor="@color/color_grey_icon"
                                                android:textSize="@dimen/_12sdp" />

                                            <TextView
                                                android:id="@+id/txt_furnishing"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentEnd="true"
                                                android:layout_centerVertical="true"
                                                android:fontFamily="@font/font_bold"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp" />
                                        </RelativeLayout>

                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/line_height"
                                            android:layout_marginVertical="@dimen/_10sdp"
                                            android:background="@color/divider" />

                                        <androidx.recyclerview.widget.RecyclerView
                                            android:id="@+id/rv_flat_amenities"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/_10sdp"
                                            tools:itemCount="1" />

                                    </LinearLayout>
                                </com.google.android.material.card.MaterialCardView>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_15sdp" />

                                <LinearLayout
                                    android:id="@+id/ll_interest_holder"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/font_bold"
                                        android:text="Interests"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12sdp" />

                                    <androidx.recyclerview.widget.RecyclerView
                                        android:id="@+id/rv_profile_interests"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_10sdp"
                                        tools:itemCount="1" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/line_height"
                                        android:layout_marginVertical="@dimen/_10sdp"
                                        android:background="@color/divider" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ll_language_holder"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/font_bold"
                                        android:text="Languages"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12sdp" />

                                    <androidx.recyclerview.widget.RecyclerView
                                        android:id="@+id/rv_profile_language"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_10sdp"
                                        tools:itemCount="1" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/line_height"
                                        android:layout_marginVertical="@dimen/_10sdp"
                                        android:background="@color/divider" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ll_deal_holder"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/font_bold"
                                        android:text="Deal-Breakers"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12sdp" />

                                    <androidx.recyclerview.widget.RecyclerView
                                        android:id="@+id/rv_flat_deals"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_10sdp"
                                        tools:itemCount="1" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/line_height"
                                        android:layout_marginVertical="@dimen/_10sdp"
                                        android:background="@color/divider" />
                                </LinearLayout>


                                <LinearLayout
                                    android:id="@+id/ll_norms"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_10sdp"
                                        android:gravity="center_vertical"
                                        android:orientation="horizontal">

                                        <ImageView
                                            android:layout_width="@dimen/_13sdp"
                                            android:layout_height="@dimen/_13sdp"
                                            android:src="@drawable/ic_flat_norms_black" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="@dimen/_10sdp"
                                            android:fontFamily="@font/font_bold"
                                            android:text="Flat Norms"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/_12sdp" />

                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/txt_norms"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_10sdp"
                                        android:fontFamily="@font/font_regular"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12sdp" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/_10sdp" />
                                </LinearLayout>

                                <com.joinflatshare.customviews.CustomButton
                                    android:id="@+id/btn_flat_share"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/button_height"
                                    android:layout_marginBottom="@dimen/_10sdp"
                                    android:background="@drawable/drawable_button_black_stroke"
                                    android:fontFamily="@font/font_regular"
                                    android:text="Recommend to a Friend"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="gone" />


                            </LinearLayout>
                        </LinearLayout>

                    </androidx.core.widget.NestedScrollView>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|end"
                        android:layout_margin="@dimen/_20sdp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/btn_edit"
                            android:layout_width="@dimen/round_card_dimen"
                            android:layout_height="@dimen/round_card_dimen"
                            android:layout_gravity="end|bottom"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:background="@drawable/drawable_circle_black_stroke">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/_15sdp"
                                android:src="@drawable/ic_edit" />
                        </LinearLayout>

                    </LinearLayout>
                </FrameLayout>
            </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

            <LinearLayout
                android:id="@+id/rl_holder_bottom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:background="@color/color_text_secondary"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_height"
                    android:background="@color/divider" />

                <RelativeLayout
                    android:id="@+id/rl_profile_bottom_default"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_60sdp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/button_height_small"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginHorizontal="@dimen/_20sdp"
                        android:orientation="horizontal">

                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/cardNotInterested"
                            android:layout_width="@dimen/button_height_small"
                            android:layout_height="match_parent"
                            app:cardBackgroundColor="@color/pink"
                            app:cardCornerRadius="@dimen/_5sdp"
                            app:cardUseCompatPadding="false">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/_10sdp"
                                android:src="@drawable/ic_cross_red" />

                        </com.google.android.material.card.MaterialCardView>

                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/cardLike"
                            android:layout_width="@dimen/button_height_small"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/_10sdp"
                            app:cardBackgroundColor="@color/color_blue_light"
                            app:cardCornerRadius="@dimen/_5sdp"
                            app:cardUseCompatPadding="false">

                            <ImageView
                                android:id="@+id/imgLike"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/_8sdp"
                                android:src="@drawable/ic_tick_blue_fat" />

                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/txt_chat_request"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/button_height_small"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginHorizontal="@dimen/_20sdp"
                        android:background="@drawable/drawable_button_black"
                        android:fontFamily="@font/font_bold"
                        android:gravity="center"
                        android:paddingHorizontal="@dimen/_10sdp"
                        android:text="Chat Request"
                        android:textColor="@color/color_text_secondary"
                        android:textSize="@dimen/_12sdp" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_profile_bottom_request"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_60sdp"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/ll_request_block"
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:gravity="center"
                        android:visibility="gone">

                        <ImageView
                            android:layout_width="@dimen/_25sdp"
                            android:layout_height="@dimen/_25sdp"
                            android:background="@android:color/transparent"
                            android:src="@drawable/ic_chat_block" />

                    </LinearLayout>


                    <TextView
                        android:id="@+id/ll_request_accept"
                        android:layout_width="@dimen/_90sdp"
                        android:layout_height="@dimen/button_height_small"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:background="@drawable/drawable_button_blue"
                        android:fontFamily="@font/font_bold"
                        android:gravity="center"
                        android:text="Accept"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/ll_request_decline"
                        android:layout_width="@dimen/_90sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:layout_toStartOf="@id/ll_request_accept"
                        android:background="@drawable/drawable_button_pink"
                        android:fontFamily="@font/font_bold"
                        android:gravity="center"
                        android:text="Reject"
                        android:textColor="@color/red"
                        android:textSize="@dimen/_12sdp" />

                </RelativeLayout>

            </LinearLayout>


        </RelativeLayout>

    </LinearLayout>


</androidx.coordinatorlayout.widget.CoordinatorLayout>
